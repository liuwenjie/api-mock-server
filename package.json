  "version": "1.0.0",
  "description": "ðŸš€ A powerful Node.js mock server with interactive dashboard that automatically creates API mocks from HAR files",
  "main": "api-mock-server.js",
  "bin": {
    "har-mock": "./api-mock-server.js"
  },
  "scripts": {
    "start": "node api-mock-server.js test.har",
